[
    {
        "name": "agbcc",
        "platform": "gba",
        "cc": "cc -E -I \"${COMPILER_DIR}\"/include -iquote include -nostdinc -undef \"$INPUT\" | \"${COMPILER_DIR}\"/bin/agbcc -quiet $COMPILER_FLAGS -o - | arm-none-eabi-as -mcpu=arm7tdmi -o \"$OUTPUT\""
    },
    {
        "name": "old_agbcc",
        "platform": "gba",
        "cc": "cc -E -I \"${COMPILER_DIR}\"/include -iquote include -nostdinc -undef \"$INPUT\" | \"${COMPILER_DIR}\"/bin/old_agbcc -quiet $COMPILER_FLAGS -o - | arm-none-eabi-as -mcpu=arm7tdmi -o \"$OUTPUT\""
    }
]
